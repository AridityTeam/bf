"VertexlitGeneric"
{
	"$baseTexture" "models\props_halloween/scary_ghost"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "3"
	"$detailblendfactor" 2
	"$detailblendmode" 1
//	"$color2" "[0 0 12]"

	"$bumpmap" "models/effects/flat_normal"
	"$nocull" "1"

	"$selfillum" "1"
	"$selfillumtint" "[.1 .7 12]"
	"$selfIllumFresnel" "1"
	"$selfIllumFresnelMinMaxExp" "[2 1 .1]"

	"$phong" "1"
	"$phongexponent" "1"
	"$phongboost" "1"	
	"$lightwarptexture" "models\player\pyro\pyro_lightwarp"
	"$phongfresnelranges"	"[0 0 20]"
//	"$basealphaenvmapmask" "1"
 

//	"$basemapalphaphongmask" "1"
//	"$envmap" "effects/cubemapper"
//	"$envmapcontrast" 1
//	"$envmaptint" "[0.0 2.0 2.5]"


	"$rimlight" "1"												
	"$rimlightexponent" "1"
	"$rimlightboost" "95"	

"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 24
		}
		"Sine"
		{
			"sinemin" ".2"
			"sinemax" ".6"
			"sineperiod" "1"
			"resultVar" "$selfillumtint[1]"
		}
		"Sine"
		{
			"sinemin" ".5"
			"sinemax" ".2"
			"sineperiod" "1.1"
			"resultVar" "$selfillumtint[0]"
		}
		"TextureScroll"
		{
			"texturescrollvar" "$detailtexturetransform"
			"texturescrollrate" -.08
			"texturescrollangle" 45
		}
	


	}
}
